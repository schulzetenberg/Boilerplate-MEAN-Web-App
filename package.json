  "lint-staged": {
    "*.js": [
      "./node_modules/.bin/eslint",
      "git add"
    ],
    "*.html": [
      "htmlhint --config .htmlhintrc",
      "js-beautify --config .jsbeautifyrc --type html -r",
      "git add"
    ],
    "*.scss": [
      "stylelint --syntax=scss"
    ],
    "*.{js,css,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "main": "./nodejs/server.js"
